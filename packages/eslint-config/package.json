{
  "name": "@vue-vine/eslint-config",
  "type": "module",
  "version": "1.1.8",
  "description": "ESLint config for Vue Vine",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "Vue",
    "Vine",
    "eslint",
    "eslintConfig"
  ],
  "exports": {
    ".": {
      "vine": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@vue-vine/eslint-plugin": "workspace:*",
    "eslint": "catalog:lint-libs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint-libs",
    "@stylistic/eslint-plugin": "catalog:lint-libs",
    "@typescript/native-preview": "catalog:compiler",
    "eslint-vitest-rule-tester": "catalog:lint-libs"
  }
}
