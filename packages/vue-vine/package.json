{
  "name": "vue-vine",
  "type": "module",
  "version": "0.5.1",
  "description": "Another style to write Vue.js",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/vue-vine"
  },
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./vite": {
      "dev": "./src/vite/index.ts",
      "import": "./dist/vite.js",
      "node": "./dist/index.js",
      "require": "./dist/vite.js"
    },
    "./macros": {
      "types": "./types/macros.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "prepublish": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "dependencies": {
    "@vue-vine/vite-plugin": "workspace:^"
  },
  "devDependencies": {
    "@vue/test-utils": "catalog:vue-libs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
