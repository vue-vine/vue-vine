{
  "name": "vue-vine",
  "type": "module",
  "version": "1.7.25",
  "description": "Another style to write Vue.js",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/vue-vine"
  },
  "exports": {
    ".": {
      "vine": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "vine": "./src/vite/index.ts",
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    },
    "./macros": {
      "types": "./types/macros.d.ts"
    },
    "./internals": {
      "types": "./types/internals.d.ts"
    },
    "./rspack": {
      "vine": "./src/rspack/index.ts",
      "types": "./dist/rspack.d.ts",
      "default": "./dist/rspack.js"
    },
    "./rsbuild": {
      "vine": "./src/rsbuild/index.ts",
      "types": "./dist/rsbuild.d.ts",
      "default": "./dist/rsbuild.js"
    },
    "./slidev": {
      "vine": "./src/slidev/index.ts",
      "types": "./types/slidev.d.ts",
      "default": "./dist/slidev.js"
    }
  },
  "files": [
    "README.md",
    "README.zh-CN.md",
    "dist",
    "types"
  ],
  "scripts": {
    "test": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "dependencies": {
    "@vue-vine/rsbuild-plugin": "workspace:*",
    "@vue-vine/rspack-loader": "workspace:*",
    "@vue-vine/vite-plugin": "workspace:*"
  },
  "devDependencies": {
    "@typescript/native-preview": "catalog:compiler",
    "@vue/test-utils": "catalog:vue-libs",
    "vitest": "catalog:vitest-libs",
    "vue": "catalog:vue-libs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
