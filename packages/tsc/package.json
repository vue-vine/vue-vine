{
  "name": "vue-vine-tsc",
  "version": "0.2.6",
  "description": "Command line typescript checker for Vue Vine",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/tsc"
  },
  "keywords": [
    "Vue",
    "Vine"
  ],
  "bin": {
    "vue-vine-tsc": "./bin/vue-vine-tsc.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@volar/language-core": "catalog:",
    "@volar/typescript": "catalog:",
    "@vue-vine/language-service": "workspace:*",
    "@vue/language-core": "catalog:"
  }
}
