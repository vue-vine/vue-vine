{
  "name": "@vue-vine/runtime-lynx",
  "type": "module",
  "version": "0.0.1",
  "description": "Vue Vine runtime for Lynx platform",
  "author": "Vue Vine Team",
  "license": "MIT",
  "homepage": "https://vue-vine.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/runtime-lynx"
  },
  "keywords": [
    "vue-vine",
    "lynx",
