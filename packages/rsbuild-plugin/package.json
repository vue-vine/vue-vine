{
  "name": "@vue-vine/rsbuild-plugin",
  "type": "module",
  "version": "1.7.19-beta.1",
  "description": "Rsbuild plugin for Vue Vine",
  "author": "Vue Vine Team",
  "license": "MIT",
  "homepage": "https://vue-vine.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-vine/vue-vine.git",
    "directory": "packages/rsbuild-plugin"
  },
  "keywords": [
    "vue-vine",
    "rsbuild",
    "plugin",
    "vue"
  ],
  "exports": {
    ".": {
      "vine": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "README.zh-CN.md",
    "dist"
  ],
  "scripts": {
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0"
  },
  "dependencies": {
    "@rsbuild/core": "catalog:rsstack",
    "@vue-vine/compiler": "workspace:*",
    "@vue-vine/rspack-loader": "workspace:*"
  },
  "devDependencies": {
    "@typescript/native-preview": "catalog:compiler"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
