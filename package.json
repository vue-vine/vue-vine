{
  "name": "vue-vine-workspace",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "description": "Yet another style to write Vue.js components",
  "author": "ShenQingchuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-vine/vue-vine.git"
  },
  "scripts": {
    "dev": "pnpm --filter @vue-vine/e2e-test run dev",
    "build": "NODE_ENV=production tsdown && pnpm --filter=\"./packages/*\" --stream build",
    "build:compiler": "NODE_ENV=production pnpm tsdown -F packages/compiler",
    "build:eslint": "tsdown -F packages/eslint-parser -F packages/eslint-plugin -F packages/eslint-config",
    "build:ext": "NODE_ENV=production tsdown -F packages/vscode-ext",
    "debug:ext": "NODE_ENV=development tsdown -F packages/vscode-ext",
    "watch:ext": "NODE_ENV=development pnpm tsdown -F packages/vscode-ext -w packages/compiler/src -w packages/language-service/src -w packages/language-service/typescript-plugin -w packages/language-server/src -w packages/vscode-ext/src",
    "test": "NODE_ENV=test pnpm --filter @vue-vine/compiler --filter @vue-vine/language-service --filter @vue-vine/language-server --filter @vue-vine/e2e-test --filter @vue-vine/eslint-parser --filter @vue-vine/nuxt --filter vue-vine run test --run",
    "test:compiler": "pnpm --filter @vue-vine/compiler run test --run",
    "test:e2e": "pnpm --filter @vue-vine/e2e-test run test --run",
    "test:eslint-parser": "pnpm --filter @vue-vine/eslint-parser run test --run",
    "test:nuxt": "pnpm --filter @vue-vine/nuxt run test --run",
    "lint": "pnpm eslint .",
    "lint:fix": "pnpm eslint . --fix",
    "docs:dev": "pnpm --filter vue-vine-docs run dev",
    "docs:build": "pnpm --filter vue-vine-docs run build",
    "docs:preview": "pnpm --filter vue-vine-docs run preview",
    "postinstall": "simple-git-hooks && tsx scripts/post-install.js",
    "clean": "tsx scripts/clean.js",
    "clean:deps": "tsx scripts/clean-deps.js && node scripts/clean-root-deps.js",
    "pack:ext": "pnpm --filter vue-vine-extension pack:ext",
    "publish:core": "pnpm --filter @vue-vine/compiler --filter @vue-vine/language-server --filter @vue-vine/language-service --filter @vue-vine/vite-plugin --filter vue-vine publish",
    "publish:eslint": "pnpm --filter @vue-vine/eslint-parser --filter @vue-vine/eslint-plugin --filter @vue-vine/eslint-config publish",
    "publish:ext": "pnpm tsx scripts/vscode-ext-pack.js publish",
    "publish:nuxt": "pnpm --filter @vue-vine/nuxt publish",
    "publish:tsc": "pnpm --filter vue-vine-tsc publish",
    "publish:cli": "pnpm --filter create-vue-vine publish",
    "publish:main": "pnpm run verify && pnpm publish:core && pnpm publish:nuxt && pnpm publish:tsc && pnpm publish:cli && pnpm publish:ext",
    "pkg-new:publish": "pnpx pkg-pr-new publish --pnpm './packages/compiler' './packages/language-service' './packages/language-server' './packages/vite-plugin' './packages/eslint-parser' './packages/eslint-plugin' './packages/eslint-config' './packages/nuxt-module' './packages/tsc' './packages/vue-vine' './packages/create-vue-vine'",
    "typecheck": "pnpm --parallel -r typecheck",
    "upgrade-template-deps": "pnpm tsx scripts/upgrade-template-deps.js",
    "local-pack-ext": "pnpm build:ext && pnpm tsx scripts/vscode-ext-pack.js pack --local",
    "verify": "pnpm build && pnpm test && pnpm typecheck && pnpm lint && pnpm lint-staged",
    "version": "pnpm changeset version && pnpm upgrade-template-deps && pnpm build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint-libs",
    "@baiwusanyu/utils-log": "catalog:utils",
    "@changesets/cli": "catalog:cli",
    "@types/node": "catalog:types",
    "@vue-vine/eslint-parser": "workspace:*",
    "del": "catalog:cli",
    "eslint": "catalog:lint-libs",
    "lint-staged": "catalog:cli",
    "playwright-chromium": "catalog:vitest-libs",
    "semver": "catalog:utils",
    "simple-git-hooks": "catalog:cli",
    "tree-kill": "catalog:cli",
    "tsdown": "catalog:cli",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "vite": "catalog:vite-ecosystem",
    "vitest": "catalog:vitest-libs",
    "vue": "catalog:vue-libs",
    "yaml": "catalog:utils"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm install && pnpm run verify && pnpm tsx scripts/pre-commit.js"
  },
  "lint-staged": {
    "*": "eslint . --fix --cache"
  }
}
