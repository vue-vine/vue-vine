{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2023"],
    "baseUrl": ".",
    "customConditions": ["dev"],
    "module": "Preserve",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
